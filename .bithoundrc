{
 "ignore": [
    "**/node_modules/**"
  ],
  "test": [
      "**/test/**",
      "**/tests/**",
      "**/spec/**",
      "**/specs/**",
      "**/*.test.ts",
      "**/*.test.js",
      "**/*.spec.js",
      "**/*.spec.ts"
    ],
  "critics": {
    "lint": {"engine": "tslint"}
  },
    "dependencies": {
      "mute": [
        "rxjs"
      ],
      "unused-ignores": [
        "babel-runtime",
        "@angular/*",
        "zone.js",
        "try-thread-sleep",
        "spawn-sync",
        "reflect-metadata",
        "meteor-typings",
        "meteor-node-stubs",
        "ionicons",
        "angular2-meteor-tests-polyfills",
        "angular2-meteor",
        "@types/*"
      ]
    }
}
